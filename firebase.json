{
  "hosting": {
    "public": "public",
    "trailingSlash": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/tag/:splat*",
        "destination": "/tags/:splat",
        "type": 301
      },
      {
        "source": "/tag",
        "destination": "/tags/",
        "type": 301
      },
      {
        "source": "/index.php",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/xilinx",
        "destination": "/tags/xilinx/",
        "type": 301
      },
      {
        "source": "/emacs",
        "destination": "/tags/emacs/",
        "type": 301
      },
      {
        "source": "/taxonomy/**",
        "destination": "/tags/",
        "type": 301
      },
      {
        "source": "/taxonomy",
        "destination": "/tags/",
        "type": 301
      },
      {
        "source": "/vhdl-2008",
        "destination": "/tags/vhdl-2008/",
        "type": 301
      },
      {
        "source": "/manual/resources/keyboard-shortcuts.pdf",
        "destination": "/resources/keyboard-shortcuts.pdf",
        "type": 301
      }
    ]
  }
}